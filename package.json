{
  "name": "prescriptions-demo",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && vite build",
    "build:client": "vite build --config vite.client.config.ts",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "wrangler d1 migrations create prescriptions-db empty-message",
    "db:migrate": "wrangler d1 migrations apply prescriptions-db --local",
    "db:deploy": "wrangler d1 migrations apply prescriptions-db --remote"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "hono": "^4.9.8",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.1",
    "@cloudflare/workers-types": "^4.20250807.0",
    "@types/node": "^24.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vite-ssr-components": "^0.3.0",
    "wrangler": "^4.28.0"
  }
}
